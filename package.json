{
  "name": "regex-doctor",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@9.1.1",
  "description": "Monitor your RegExp consumption and provide suggestions to improve performance.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/regex-doctor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/regex-doctor.git"
  },
  "bugs": "https://github.com/antfu/regex-doctor/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./register": {
      "types": "./dist/register.d.ts",
      "import": "./dist/register.mjs",
      "require": "./dist/register.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": "./bin.mjs",
  "files": [
    "bin.mjs",
    "dist"
  ],
  "scripts": {
    "build": "nr js:build && nr ui:build",
    "start": "nr js:build && nr -C ui build:doctor && nr -C test/fixtures/vite build && nr dev",
    "dev": "nr -C ui dev",
    "js:build": "unbuild",
    "ui:build": "nr -C ui build",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "pnpm -r typecheck",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "error-stack-parser-es": "^0.1.4",
    "exit-hook": "^4.0.0",
    "get-port-please": "^3.1.2",
    "h3": "^1.11.1",
    "launch-editor": "^2.6.1",
    "mrmime": "^2.0.0",
    "open": "^10.1.0",
    "picocolors": "^1.0.1",
    "trace-record": "^0.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.0",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.8",
    "@iconify-json/carbon": "^1.1.33",
    "@iconify-json/ph": "^1.1.13",
    "@types/node": "^20.12.12",
    "bumpp": "^9.4.1",
    "eslint": "^9.2.0",
    "esno": "^4.7.0",
    "js-base64": "^3.7.7",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.1.1",
    "regex-doctor": "workspace:*",
    "rimraf": "^5.0.7",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "vue": "^3.4.27"
  },
  "pnpm": {
    "overrides": {
      "nitropack": "2.8.1"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
